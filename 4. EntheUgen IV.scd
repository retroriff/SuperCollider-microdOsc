(
~te1 = {
    Gbman2DL.ar(
        minfreq: [400, 390],
        maxfreq: 400,
        x0: LFPulse.kr(~tempo.kr / 8).range(0.5, 3),
        y0: LFPulse.kr(~tempo.kr / 8).range(0, 4),
        mul: LFSaw.kr(~tempo.kr * 8).range(0.5, 1)
    )
}
)

(
~te2 = {
    var sig1, sig2;
    sig1 = Gbman2DN.ar(
        minfreq: 600 !2 * SinOscFB.kr(~tempo.kr * 8, feedback: 0.5).range(0.8, 3),
        maxfreq: 150 * SinOsc.kr(~tempo.kr / 2).range(0.8, 1.2),
        x0: 1.2,
        y0: 2.5,
        mul: LFSaw.kr(~tempo.kr / 4).range(0, 0.3)
    );
    sig2 = LFSaw.ar(~tempo.kr / 4).range(0, 1.5);
    sig1 * sig2;
}
)

(
~te3 = {
    Gbman2DN.ar(
        minfreq: 800 * LFPulse.kr(~tempo.kr / 8).range(2, 2),
        maxfreq: 200,
        x0: 1.2,
        y0: 2,
        mul: LFSaw.kr(~tempo.kr).range(0, 0.8)
    ) !2;
}
)

